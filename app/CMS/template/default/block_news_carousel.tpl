<div id="carousel" class="carousel slide carousel-fade <!--class-->" data-ride="carousel">
	<div class="carousel-inner">
<!--loop:start-->
		<div class="carousel-item <!--news_active-->">
			<img src="<!--news_image-->" class="d-block w-100" alt="<!--news_subject-->">
			<div class="carousel-caption d-none d-md-block">
				<a href="<!--news_link-->" target="_blank"><h5><!--news_subject--></h5></a>
			</div>
		</div>
<!--loop:end-->
	</div>
	<a class="carousel-control-prev" href="#carousel" data-slide="prev">
		<span class="carousel-control-prev-icon" aria-hidden="true"></span>
		<span class="sr-only">Previous</span>
	</a>
	<a class="carousel-control-next" href="#carousel" data-slide="next">
		<span class="carousel-control-next-icon" aria-hidden="true"></span>
		<span class="sr-only">Next</span>
	</a>
</div>